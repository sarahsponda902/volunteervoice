<script type="text/javascript">
var theLocations = <%=array_or_string_for_javascript(@locations)%>;
var allTheLocations = <%= array_or_string_for_javascript(ALLCOUNTRIES)%>;


$(document).ready(function() {
	//hide the "others" category of countries. Leasie doesn't want to use it right now
	$("#others").hide();
	
	//show the country if there is a program in that country
    var count = 0;
    for (count = 0; count <= allTheLocations.length; count++)
    {
        $("#"+allTheLocations[count]).hide();
        if (jQuery.inArray(allTheLocations[count], theLocations) != -1)
         {
			$("#"+allTheLocations[count]).show();
		 }
    }


//qMark popup jQuery
	$('a.qMark-window').click(function() {

                //Getting the variable's value from a link 
		var qMarkBox = $(this).attr('href');

		//Fade in the Popup
		$(qMarkBox).fadeIn(300);

		//Set the center alignment padding + border see css style
		var popMargTop = ($(qMarkBox).height() + 24) / 2; 
		var popMargLeft = ($(qMarkBox).width() + 24) / 2; 

		$(qMarkBox).css({ 
			'margin-top' : -popMargTop,
			'margin-left' : -popMargLeft
		});

		// Add the mask to body
		$('body').append('<div id="mask" style="display:block; opacity:.5;filter: alpha(opacity=50);"></div>');
		$('#mask').fadeIn(300);

		return false;
	});

	// When clicking on the button close or the mask layer the popup closed
	$('a.close, #mask').live('click', function() { 
	  $('#mask , .qMark-popup').fadeOut(300 , function() {
		$('#mask').remove();  
	}); 
	return false;
	});


});

</script>
