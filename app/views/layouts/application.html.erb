		<script type="text/javascript">

		//Login Window Jquery
		$(document).ready(function() {

			$('a.login-window').click(function() {

		                //Getting the variable's value from a link 
				var termsBox = $(this).attr('href');

				//Fade in the Popup
				$(termsBox).fadeIn(300);

				//Set the center alignment padding + border see css style
				var popMargTop = ($(termsBox).height() + 24) / 2; 
				var popMargLeft = ($(termsBox).width() + 24) / 2; 

				$(termsBox).css({ 
					'margin-top' : -popMargTop,
					'margin-left' : -popMargLeft
				});

				// Add the mask to body
				$('body').append('<div id="mask" style="display:block; opacity:.5; filter: alpha(opacity=50);"></div>');
				$('#mask').fadeIn(300);

				return false;
			});

			// When clicking on the button close or the mask layer the popup closed
			$('#mask').live('click', function() { 
			  $('#mask').fadeOut(300);
			  $('#login-box').fadeOut(300);
			  $('#mask').remove();
			  return false;
			});
			$('a.close').live('click', function() { 
			  $('#mask').fadeOut(300);
			  $('#login-box').fadeOut(300);
			  $('#mask').remove(); 
			  return false;
			});
			
			
			//Account Request JQuery
			$('a.request-window').click(function() {

		                //Getting the variable's value from a link 
				var termsBox = $(this).attr('href');

				//Fade in the Popup
				$(termsBox).fadeIn(300);

				//Set the center alignment padding + border see css style
				var popMargTop = ($(termsBox).height() + 24) / 2; 
				var popMargLeft = ($(termsBox).width() + 24) / 2; 

				$(termsBox).css({ 
					'margin-top' : -popMargTop,
					'margin-left' : -popMargLeft
				});

				// Add the mask to body
				$('body').append('<div id="mask" style="display:block; opacity:.5; filter: alpha(opacity=50);"></div>');
				$('#mask').fadeIn(300);

				return false;
			});

			// When clicking on the button close or the mask layer the popup closed
			$('#mask').live('click', function() { 
			  $('#mask').fadeOut(300);
			  $('#request-box').fadeOut(300);
			  $('#mask').remove();
			  return false;
			});
			$('a.close').live('click', function() { 
			  $('#mask').fadeOut(300);
			  $('#request-box').fadeOut(300);
			  $('#mask').remove(); 
			  return false;
			});
			
			
			//Org Login Window Jquery
			$('a.org-login-window').click(function() {

		                //Getting the variable's value from a link 
				var termsBox = $(this).attr('href');

				//Fade in the Popup
				$(termsBox).fadeIn(300);

				//Set the center alignment padding + border see css style
				var popMargTop = ($(termsBox).height() + 24) / 2; 
				var popMargLeft = ($(termsBox).width() + 24) / 2; 

				$(termsBox).css({ 
					'margin-top' : -popMargTop,
					'margin-left' : -popMargLeft
				});

				// Add the mask to body
				$('body').append('<div id="mask" style="display:block; opacity:.5; filter: alpha(opacity=50);"></div>');
				$('#mask').fadeIn(300);

				return false;
			});

			// When clicking on the button close or the mask layer the popup closed
			$('a.close', '#mask').live('click', function() { 
			  $('#mask').fadeOut(300);
			  $('#org-login-box').fadeOut(300);
			  $('#mask').remove();  
			  return false;
			});
			$('a.close').live('click', function() { 
			  $('#mask').fadeOut(300);
			  $('#org-login-box').fadeOut(300);
			  $('#mask').remove(); 
			  return false;
			});
			
			
			
			////BROWSER SNIFFING!!! AAHH!!!!
			////// some things looked funky in different browsers
			
			//Firefox - account request's right column is too low
			if ( $.browser.mozilla == true ) {
				$("#request-right-column").css("margin-top", "-333px");
			}
			
			//Opera - select box would not render formatting, so took it out
			if ($.browser.opera) {
				$(".select-div").css("border", "none");
				$(".select-div").css("overflow", "visible");				
			}
			
			//Android - org's login image is too low & find a program dropDown is too high
			var ua = navigator.userAgent.toLowerCase();
			var isAndroid = ua.indexOf("android") > -1;
			if (isAndroid) {
				$("#orgLogin-image").css("margin-top", "-15px");
				$("#program_drop_down").css("top", "223px");
			}
			
			//iOS - find a program dropDown is too high
			var agentID = ua.match(/(iphone|ipod|ipad)/);
			if (agentID) {
				$("#program_drop_down").css("top", "183px");
			}
				
		});
	</script>

